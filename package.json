{
  "name": "@o2v/a-cache",
  "version": "0.0.1-beta.5",
  "description": "a tool for cache and discache of the result of a call.  ",
  "main": "lib/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "lib",
    "typings"
  ],
  "scripts": {
    "dist": "SET NODE_ENV=production&& rollup -c ./rollup.config.js",
    "dist:dev": "rollup -c ./rollup.config.js",
    "test": "npm run dist:dev && ava",
    "publish2npm": "npm run dist && npm run test && npm publish --access=public"
  },
  "author": "herbluo",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.51",
    "ava": "1.0.0-beta.6",
    "fs-extra": "^7.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HerbLuo/a-cache.git"
  },
  "keywords": [
    "cache",
    "disable",
    "cache",
    "decorators"
  ],
  "ava": {
    "files": [
      "test/*.js",
      "!test/utils/*.js"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {}
}
