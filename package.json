{
  "name": "a-cache",
  "version": "0.0.1-beta.1",
  "description": "a tool for cache and discache of the result of a call.  ",
  "main": "lib/index.js",
  "typings": "index.ts",
  "files": [
    "lib",
    "src",
    "index.ts"
  ],
  "scripts": {
    "dist": "SET NODE_ENV=production&& rollup -c ./rollup.config.js",
    "dist:dev": "rollup -c ./rollup.config.js",
    "test": "npm run dist:dev && ava"
  },
  "author": "herbluo",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.51",
    "ava": "1.0.0-beta.6",
    "rollup": "^0.62.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HerbLuo/a-cache.git"
  },
  "keywords": [
    "cache",
    "disable",
    "cache",
    "decorators"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
